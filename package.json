{
  "name": "kengurukleo",
  "version": "1.0.0",
  "description": "Recipes",
  "engines": {
    "node": ">=8.4.x",
    "npm": ">=5.4.x"
  },
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run build:prod",
    "start": "forever ./server/app.js",
    "server:dev": "nodemon --exec node ./server/app.js --watch server",
    "server:prod": "forever ./server/app.js",
    "start:dev": "webpack-dev-server --mode=development --progress --hot --inline --host 0.0.0.0 --port 3000 --devtool inline-source-map",
    "build:prod": "rimraf './dist' && webpack -p --mode=production --config ./config/webpack.config.prod.js",
    "build:dev": "rimraf './dist' && webpack --mode=development --devtool inline-source-map",
    "bundle:analyze": "rimraf './dist' && webpack -p --mode=production --config ./config/webpack.config.analyze.js",
    "test": "jest"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./config/enzyme.setup.js",
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.(css|scss)$": "jest-css-modules-transform"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "^.+\\.(svg|jpe?g)$": "identity-obj-proxy"
    },
    "transformIgnorePatterns": [
      "node_modules"
    ],
    "collectCoverage": true
  },
  "dependencies": {
    "autoprefixer": "^8.4.1",
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.2",
    "classname": "0.0.0",
    "compression": "^1.7.2",
    "compression-webpack-plugin": "^1.1.11",
    "copy-webpack-plugin": "^4.5.1",
    "cors": "^2.8.4",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-babel": "^5.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-relay": "^0.0.21",
    "express": "^4.16.3",
    "express-history-api-fallback": "^2.2.1",
    "express-static-gzip": "^0.3.2",
    "file-loader": "^1.1.11",
    "forever": "^0.15.3",
    "history": "^4.7.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "jest-css-modules-transform": "^2.0.0",
    "jwt-simple": "^0.5.1",
    "lodash.get": "^4.4.2",
    "mini-css-extract-plugin": "^0.4.0",
    "morgan": "^1.9.0",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.3",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "postcss-loader": "^2.1.5",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-css-themr": "^2.1.2",
    "react-dom": "^16.3.2",
    "react-modal": "^3.4.4",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.1",
    "shortid": "^2.2.8",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "watchman": "^1.0.0",
    "webpack": "^4.7.0",
    "webpack-bundle-analyzer": "^2.11.3",
    "webpack-cli": "^2.1.2",
    "webpack-dev-server": "^3.1.4",
    "winston": "^2.4.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kengurukleo/recipes.git"
  },
  "author": "Konstantin Yemelianov",
